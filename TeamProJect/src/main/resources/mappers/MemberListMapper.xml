<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itwillbs.mappers.MemberListMapper">

	<!-- 회원 목록 조회 -->
	<select id = "getmemberList" resultType="com.itwillbs.domain.MemberDTO">
		SELECT USER_ID
		     , USER_TYPE
		     , PASSWORD
		     , USER_NAME
		     , AES_DECRYPT(UNHEX(JUMIN),'itwill') AS JUMIN
		     , USER_PHONENUMBER
		     , USER_ADDRESS
		  FROM MEMBER
	</select>

</mapper> 