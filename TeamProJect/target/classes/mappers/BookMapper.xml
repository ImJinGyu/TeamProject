<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itwillbs.mappers.adminMapper">
	<select id="listReservation" resultType="com.itwillbs.domain.BookDTO">
		SELECT RES_NUMBER
			 , USER_ID
			 , PEN_ID
			 , CHECK_IN
			 , CHECK_OUT
			 , RES_TIME
